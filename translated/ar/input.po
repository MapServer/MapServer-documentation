msgid "Access is available in MapServer through OGR."
msgstr ""

# 6dfa865696d04fb0b5f4d8dcc43c7bca
# bfeb5c8c5d0843c3b2d6d5768c279ace
# f8187ff71fe24830929f1acd6c297f17
# a9ba4eb2a6ca45f7ad92c968ab3b7fa0
#: ../../../en/input/vector/dgn.txt:20 ../../../en/input/vector/gml.txt:47
