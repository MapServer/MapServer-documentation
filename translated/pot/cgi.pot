msgstr ""

#: ../../../en/cgi/controls.txt:66
# eed1c8a83ee84c6f9d79fef62788c0ec
msgid "Coordinates (in pixels) of a box drag in the image. Most often used in conjunction with Java based front ends to the MapServer."
msgstr ""

#: ../../../en/cgi/controls.txt:74
# 120f575e82764ffe8497a4d298676d9f
msgid "IMGEXT [minx] [miny] [maxx] [maxy]"
msgstr ""

#: ../../../en/cgi/controls.txt:73
# 31963f1927554b2195bd798102749bb7
