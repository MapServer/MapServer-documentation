#: ../../../en/cgi/controls.txt:50
# 6ef879750e94425e880a3b584b1e618f
msgid "ID [id-string]"
msgstr ""

#: ../../../en/cgi/controls.txt:49
# 62f2f2139a0f46429b2e1ccb9a00034a
msgid "By default MapServer generates a uniq session id based on system time and process id. This parameter overwrites the default."
msgstr ""

#: ../../../en/cgi/controls.txt:60
# befc3a8e07d84209bc18463d74511a02
