#: ../../../en/ogc/wms_server.txt:640
# 136c8fbac8e348108249f92111b9f406
msgid "e.g, the URL wmsmap?mode=map will be rewritten as"
msgstr ""

#: ../../../en/ogc/wms_server.txt:643
# b26cb3f45a2c4c2c841475bc20829d22
msgid "::"
msgstr ""

#: ../../../en/ogc/wms_server.txt:643
# 60f06f4c2da84b32ad8477ba6c25afed
msgid "/cgi-bin/mapserv?map=/home/www/mapserverstuff/mymapfile.map&mode=map"
msgstr ""

#: ../../../en/ogc/wms_server.txt:645
# 3393fb3b8000403c8d75a01965e825cb
msgid "If just the URL wmsmap is given (without any parameters) a page not found error will show up as that does not match the wmsmap? expression."
msgstr ""

#: ../../../en/ogc/wms_server.txt:653
# 732f1ace13724035a028f44c9ae8d50d
msgid "Apache environment variables - MS_MAPFILE"
msgstr ""

#: ../../../en/ogc/wms_server.txt:655
# 4a0c4b4ea0794fff81333eb236501bdb
msgid "A default mapfile can be specified using the MS_MAPFILE environment variable:"
msgstr ""

#: ../../../en/ogc/wms_server.txt:672
# 17854dec4b9b421c992444e28c9b6555
msgid "Apache SetEnvIf"
msgstr ""

#: ../../../en/ogc/wms_server.txt:674
# f4ecef359e0e4cb6a049ac080eae9377
msgid "Another option is to use the \"setenvif\" feature of Apache: use symbolic links that all point to a same mapserv binary, and then for each symbolic link test the URL, and set the MAP environment accordingly."
msgstr ""

