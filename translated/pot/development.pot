msgstr ""

#: ../../../en/development/rfc/ms-rfc-103.txt:43
# 38b4c47b2808435bbf41960f1bc243ea
msgid "This RFC proposes to add an ENCODING keyword at the LAYER level. Layer datasources that have an ENCODING set, and different than UTF8, will have its attributes converted to UTF8 in the GetShape and GetNextShape vtable wrappers."
msgstr ""

#: ../../../en/development/rfc/ms-rfc-103.txt:48
# e1e747765bac41109538d4a6dcc41a08
msgid "2.1 Implications"
msgstr ""

#: ../../../en/development/rfc/ms-rfc-103.txt:50
# 2cac75989b0244e1970494f9fcdc1704
msgid "The core of MapServer can forget about handling encodings, and will treat all strings as UTF8."
msgstr ""

#: ../../../en/development/rfc/ms-rfc-103.txt:52
# f42e6708e75447b5ad02f5bfccb8a4cf
msgid "All text documents ( e.g. capabilities, getfeature, query, etc.. ) will be emitted as UTF8."
msgstr ""

#: ../../../en/development/rfc/ms-rfc-103.txt:54
# 2d763666352e4f40b725ebb28e3728d9
msgid "There will be no need to specify encoding metadata entries in the mapfile in order to produce somewhat correct ogc documents."
msgstr ""

