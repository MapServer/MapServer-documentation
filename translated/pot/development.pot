msgstr ""

#: ../../../en/development/rfc/ms-rfc-103.txt:60
# 246d5d21ed0448b8a0995e73da3a1c4f
msgid "This RFC has some notable backwards incompatibility issues for users who were treating non UTF8 or ASCII datasets:"
msgstr ""

#: ../../../en/development/rfc/ms-rfc-103.txt:63
# 1b9e737460364ad68cfd18d0532afb94
msgid "Mapfiles themselves will be expected to be UTF8-encoded. Non UTF8-encoded mapfiles will need to be iconv'd to utf8."
