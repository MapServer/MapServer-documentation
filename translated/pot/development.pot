msgid "Feature cache"
msgstr ""

#: ../../../en/development/rfc/ms-rfc-22a.txt:45
# ce52f256fffb42a78084ab5426b2ba40
msgid "Providing default layer style based on external style information, or attribute based styling"
msgstr ""

#: ../../../en/development/rfc/ms-rfc-22a.txt:47
# 924cf578e4eb448db881447295e9d7df
msgid "Providing custom data filters"
msgstr ""

