msgstr ""

#: ../../en/input/vector/oracle.txt:259
# be414ab522ea49f694e690dd57b6f0c9
msgid "This indicates MapServer to use a internal SQL that it's compatible with Oracle 8i version."
msgstr ""

#: ../../en/input/vector/oracle.txt:263
