msgstr ""

#: ../../../en/mapfile/label.txt:92
# 3b27ddd37ad84a7c9aeba26d03bbd041
msgid "Padding, in pixels, around labels. Useful for maintaining spacing around text to enhance readability. Available only for cached labels. Default is 0."
msgstr ""

#: ../../../en/mapfile/label.txt:119
#: ../../../en/mapfile/style.txt:144
# 7d1a57d148e8468cab72360d9893658e
# 2226631321224ba3a0c4e4b237d10ab5
msgid "COLOR [r] [g] [b] | [attribute]"
