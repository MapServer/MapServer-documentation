msgstr ""

#: ../../../en/mapfile/label.txt:434
# c21eee18c5b0417a9d03d77022b21962
msgid "SIZE [double]|[tiny|small|medium|large|giant]|[attribute]"
msgstr ""

#: ../../../en/mapfile/label.txt:406
