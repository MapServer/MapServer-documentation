#: ../../../en/mapfile/template.txt:8
# 4cc3280dc37a4298840cefc54a35e121
msgid "Templating"
msgstr ""

#: ../../../en/mapfile/template.txt:10
# 1c0ccca5a28a47aa838c4b38d6c36443
msgid "Frank Koormann"
msgstr ""

#: ../../../en/mapfile/template.txt:11
