#: ../../../en/ogc/sos_server.txt:925
msgid "**sos_procedure**"
msgstr ""

# 38c126c643be4a66b408b09d854111e1
#: ../../../en/ogc/sos_server.txt:927
msgid "*Description:* (Required) Normally a sensor unique id.  One per layer:"
