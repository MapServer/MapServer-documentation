# 4808d6361056412a814b676dfc0757e2
msgid "Java and C-Sharp"
msgstr ""

#: ../../en/development/rfc/ms-rfc-24.txt:540
#: ../../en/development/rfc/ms-rfc-24-FIRSTDRAFT.txt:377
# 9963d64a292c441f87d68e0b7efb3993
# 78860a9862604c1caf9e7963d3c2da42
msgid "SWIG-Java and SWIG-CSharp share a common ground and are therefore very similar. The names of SWIG-Java constructs can be roughly translated into their C-Sharp equivalents by changing the *java* prefix into *cs* (i.e. *javacode* in *cscode*, *javaconstruct* in *csconstruct* and *javaout* in *csout*)."
msgstr ""

#: ../../en/development/rfc/ms-rfc-24.txt:545
# 5a8fd2ce36e34f79a7ba43a2692b05de
