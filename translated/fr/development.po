#: ../../en/development/rfc/ms-rfc-35.txt:133
# b6959c15eee140b0b1b71c03233165a1
msgid "No changes are anticipated in mapscript as it already has metadata setting methods."
msgstr ""

#: ../../en/development/rfc/ms-rfc-35.txt:139
# 13319c28233b464fa860b88da4f28f5d
msgid "Implementation of this feature should restore backward compatible default behavior in MapServer 5 with MapServer 4.x. No other compatability issues are anticipated."
msgstr ""

#: ../../en/development/rfc/ms-rfc-35.txt:146
# 6e9bf617a77a49d9be0ef8fdf1e8acee
msgid "http://trac.osgeo.org/mapserver/ticket/2427"
msgstr ""

#: ../../en/development/rfc/ms-rfc-35.txt:151
