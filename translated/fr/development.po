# 681a6bf70f544deba6a2d6f8e86636ac
msgid "Metadata Mapping"
msgstr ""

#: ../../en/development/rfc/ms-rfc-41.txt:56
# 146a040682ef47768b7a9d6c9d245d47
msgid "The current WCS metadata items are tightly related to the WCS 1.0 protocol, while the WCS 1.1 protocol used a substantially different form and conventions for service, and coverage descriptions as well as for the getcoverage request. The following table indicates which WCS metadata items are mapped to what coverage XML elements in WCS 1.0 and WCS 1.1::"
msgstr ""

#: ../../en/development/rfc/ms-rfc-41.txt:77
# 1f34afa6f37142a18f0d93fd48c778c5
msgid "URNs / Coordinate Systems and Axis Orientation"
msgstr ""

#: ../../en/development/rfc/ms-rfc-41.txt:79
# 7b41db3329524c899aff339209f588a1
msgid "WCS 1.1 uses URNs like \"urn:ogc:def:crs:EPSG::4326\" or \"urn:ogc:def:crs:OGC::CRS84\".  In addition the WCS protocol is required to honour EPSG axis conventions when using coordinate systems within the EPSG authority space.  This means, for instance, that any coordinates in the urn:ogc:def:crs:EPSG::4326 coordinate system must be provided in lat,long ordering instead of the conventional long,lat."
msgstr ""

#: ../../en/development/rfc/ms-rfc-41.txt:86
# fb852f64481348feb96f7ec227e2e422
msgid "In order to implement these requirements, several changes are planned:"
msgstr ""

#: ../../en/development/rfc/ms-rfc-41.txt:88
# f684ff48438a4c10bc2c40fae9eb6934
