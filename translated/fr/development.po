msgid "Feature cache"
msgstr ""

#: ../../../en/development/rfc/ms-rfc-22a.txt:44
# ab1805c1a3a345f5addc44b2a1d281d3
msgid "Providing default layer style based on external style information, or attribute based styling"
msgstr ""

#: ../../../en/development/rfc/ms-rfc-22a.txt:46
