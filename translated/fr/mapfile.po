#: ../../en/mapfile/expressions.txt:749
msgid "Spatial functions that return a number (GEOS)"
msgstr "Fonctions spatiales qui retournent un nombre (GEOS)"

# 1b654dfbf18b43d5a101f6efd00c2150
#: ../../en/mapfile/expressions.txt:754
msgid "area ( shape1 )"
msgstr "area ( forme1 )"

# f7d3f8f51ac44615ab6a5da57b7fdea3
#: ../../en/mapfile/expressions.txt:756
msgid "returns the area of shape1"
msgstr "retourne l'aire de forme1"

# b4057e08dde548aab2f7c62015c0409c
#: ../../en/mapfile/expressions.txt:762
msgid "Spatial functions that return a shape (GEOS)"
msgstr "Fonctions spatiales qui retournent une forme (GEOS)"

# 4a7c204a2e624fab9a65ca572e50c6a4
#: ../../en/mapfile/expressions.txt:767
msgid "fromtext ( \"String1\" )"
msgstr "fromtext ( \"Chaine1\" )"

# 2223f5e5661642578c72f64c68f8caa0
#: ../../en/mapfile/expressions.txt:769
msgid "returns the shape corresponding to String1 (WKT - well known text)"
msgstr "retourne la forme correspondante à la Chaine1 (WKT - well known text)"

# daf4f1b5bd1544b2883ca7509cf761c0
#: ../../en/mapfile/expressions.txt:780
msgid "buffer (shape1 , n1 )"
msgstr "buffer (forme1 , n1 )"

# f56ad71b8a684cf4ad42c1a97b8588d6
