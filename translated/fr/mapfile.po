msgid "Arithmetic operations and functions that return a number"
msgstr "Opérations arithmétiques et fonctions qui retournent un  nombre"

# f70d5fea93bd44808fc69f0286bb793e
#: ../../en/mapfile/expressions.txt:695
msgid "round ( n1 , n2 )"
msgstr "round ( n1 , n2 )"

# 06057d09e82b42eb94aaf9db048c1e74
#: ../../en/mapfile/expressions.txt:697
msgid "returns n1 rounded to a multiple of n2: n2 * round(n1/n2)"
msgstr "retourne n1 arrondi à un multiple de n2: n2 * round(n1/n2)"

# 004ce4f7885347a1b507da836be3dbd4
#: ../../en/mapfile/expressions.txt:701
msgid "n1 + n2"
msgstr "n1 + n2"

# 85d5420c36254d55a3307ca89f3e4a67
#: ../../en/mapfile/expressions.txt:703
msgid "returns the sum of n1 and n2"
msgstr "retourne la somme de n1 et n2"

# 0d671155fc4b47e5aa3a40a8e61159b0
#: ../../en/mapfile/expressions.txt:705
msgid "n1 - n2"
msgstr "n1 - n2"

# 3a018dfbc27b4358b04805efcc9850e7
#: ../../en/mapfile/expressions.txt:707
msgid "returns n2 subtracted from n1"
msgstr "retourne n2 soustrait de n1"

# 21a8d216ed47431494673acdd0e54b2d
#: ../../en/mapfile/expressions.txt:709
msgid "n1 \\* n2"
msgstr "n1 \\* n2"

# 5f0be1d878984dc28322b692b6731b64
#: ../../en/mapfile/expressions.txt:711
msgid "returns n1 multiplicated with n2"
msgstr "retourne n1 multiplié par n2"

# 1df0f7044ad240859b35a5049c61bb2a
#: ../../en/mapfile/expressions.txt:713
msgid "n1 / n2>"
msgstr "n1 / n2>"

# f842d62326304e1a85e1a9596df961a9
#: ../../en/mapfile/expressions.txt:715
