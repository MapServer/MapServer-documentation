msgstr "retourne n1 divisé par n2"

# 7b4fdb85670e4ee4b4948872cae11b79
#: ../../en/mapfile/expressions.txt:717
msgid "-n1"
msgstr "-n1"

# fca28c2175964a3490d79317c018a501
#: ../../en/mapfile/expressions.txt:719
