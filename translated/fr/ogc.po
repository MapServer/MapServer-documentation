msgid "CRS:27 (NAD27 longitude-latitude)"
msgstr ""

#: ../../en/ogc/wms_server.txt:765
# 36db79952cc24a58894e33cc825874ad
msgid "AUTO2:420001 (WGS 84 / Auto UTM )"
msgstr ""

#: ../../en/ogc/wms_server.txt:766
# 52f0ac65497446258d5d7755c6c8ba35
msgid "AUTO2:420002 (WGS 84 / Auto Tr. Mercator)"
msgstr ""

#: ../../en/ogc/wms_server.txt:767
# 54ef71becd554fd68f426d024e015f81
msgid "AUTO2:420003 (WGS 84 / Auto Orthographic)"
msgstr ""

#: ../../en/ogc/wms_server.txt:768
