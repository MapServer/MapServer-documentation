#: ../../../en/ogc/wms_client.txt:203
# 28f54caf7c5e4756a3be30cfbec36fac
msgid "Required Layer Parameters and Metadata"
msgstr ""

#: ../../../en/ogc/wms_client.txt:206
# d9a01f3284114857a666f012dd1e5897
msgid "**CONNECTIONTYPE WMS**"
msgstr ""

#: ../../../en/ogc/wms_client.txt:208
# d573a8feefcb47cb9399185b335c1850
msgid "**CONNECTION** - this is the remote server's online resource URL, just the base URL without any of the WMS parameters. The server version, image format, layer name, etc. will be provided via metadata, see below."
msgstr ""

#: ../../../en/ogc/wms_client.txt:215
# 3e0e4c4e573f48468023df3c4a5c61bb
msgid "Note that if the CONNECTION parameter value is not set the the value of the \"wms_onlineresource\" metadata will be used. If both CONNECTION and \"wms_onlineresource\" are set then the \"wms_onlineresource\" metadata takes precedence."
msgstr ""

#: ../../../en/ogc/wms_client.txt:219
# cba21dd0e6a049f6b5abf625e1d5745d
msgid "**\"wms_format\" metadata** - the image format to use in GetMap requests."
msgstr ""

#: ../../../en/ogc/wms_client.txt:224
# 76cf833e257d43b4a84fa247cdc32e1b
msgid "If wms_formatlist is provided then wms_format is optional and MapServer will pick the first supported format in wms_formatlist for use in GetMap requests. If both wms_format and wms_formatlist are provided then wms_format takes precedence. Also note that WMS Servers only advertize supported formats that are part of the GD/GDAL libraries."
msgstr ""

#: ../../../en/ogc/wms_client.txt:230
# 02b13ba7d0544cd8988791982368c6e4
msgid "**\"wms_name\" metadata** - comma-separated list of layers to be fetched from the remote WMS server. This value is used to set the LAYERS and QUERY_LAYERS WMS URL parameters."
msgstr ""

#: ../../../en/ogc/wms_client.txt:234
# 0891724dc9274c86985f0c1230667f8c
msgid "**\"wms_server_version\" metadata** - the version of the WMS protocol supported by the remote WMS server and that will be used for issuing GetMap requests."
msgstr ""

#: ../../../en/ogc/wms_client.txt:238
# e72751401ad14bab928935fe9cf2fdf4
msgid "**\"wms_srs\" metadata** - space-delimited list of EPSG projection codes supported by the remote server. You normally get this from the server's capabilities output. This value should be upper case (EPSG:4236.....not epsg:4236) to avoid problems with case sensitive platforms. The value is used to set the SRS WMS URL parameter."
msgstr ""

#: ../../../en/ogc/wms_client.txt:245
# 74c9f590a2cf487a9af15634071baf52
msgid "Optional Layer Parameters and Metadata"
msgstr ""

#: ../../../en/ogc/wms_client.txt:247
# a9337ed6a56c45838e734f6737f014db
msgid "**MINSCALE, MAXSCALE** - if the remote server's capabilities contains a ScaleHint value for this layer then you might want to set the MINSCALE and MAXSCALE in the LAYER object in the mapfile. This will allow MapServer to request the layer only at scales where it makes sense"
msgstr ""

#: ../../../en/ogc/wms_client.txt:252
# 5b95642c838547dc9224a03840b91529
msgid "**PROJECTION object** - it is optional at this point. MapServer will create one internally if needed.  Including one may allow MapServer to avoid looking up a definition in the PROJ.4 init files."
msgstr ""

#: ../../../en/ogc/wms_client.txt:256
# ee4216d99374491bbf89c0d03594898c
msgid "**\"wms_auth_username\" metadata** - msEncrypt-style authorization string.  Empty strings are also accepted."
msgstr ""

#: ../../../en/ogc/wms_client.txt:266
# 2c9c80b8cc1d4ee4acb54894ea0a2fd7
msgid "**\"wms_auth_type\" metadata** - Authorization type.  Supported types include:"
msgstr ""

#: ../../../en/ogc/wms_client.txt:269
#: ../../../en/ogc/wms_client.txt:350
# 341a9e66d6f84522a81c98223c0c7a64
# b0c828f7197840ac84171ff2757ddb61
msgid "basic"
msgstr ""

#: ../../../en/ogc/wms_client.txt:270
#: ../../../en/ogc/wms_client.txt:351
# 47ec30eb90474ef2aa231e6fd4328b28
# b22cc0c45dbe4a77a6993ab9967892ec
msgid "digest"
msgstr ""

#: ../../../en/ogc/wms_client.txt:271
#: ../../../en/ogc/wms_client.txt:352
# 3f3411e7c3524bc0b0202d39f628c6e7
# 7d2add1d79ce468b8267cd4aa541d276
msgid "ntlm"
msgstr ""

#: ../../../en/ogc/wms_client.txt:272
#: ../../../en/ogc/wms_client.txt:353
# d9761b780ec64a39a592f4ba4a72716b
# 87d04b28b1b64e5a938e677862bd4bb0
msgid "any (the underlying http library picks the best among the opotions supported by the remote server)"
msgstr ""

#: ../../../en/ogc/wms_client.txt:273
#: ../../../en/ogc/wms_client.txt:354
# df6a2290e69c4745b895a946909d4cd5
# 8114a00d4bec4898b59ad65c00933c12
msgid "anysafe (the underlying http library picks only safe methods among the options supported by the remote server)"
msgstr ""

#: ../../../en/ogc/wms_client.txt:281
# b1b76146af504e8793adf5fb61a52b4b
msgid "**\"wms_connectiontimeout\" metadata** - the maximum time to wait for a remote WMS layer to load, set in seconds (default is 30 seconds). This metadata can be added at the layer level so that it affects only that layer, or it can be added at the map level (in the web object) so that it affects all of the layers. Note that wms_connectiontimeout at the layer level has priority over the map level."
msgstr ""

#: ../../../en/ogc/wms_client.txt:295
# 34cb1cd147db4aebafadacadddf28507
msgid "**\"wms_exceptions_format\" metadata** - set the format for exceptions (as of MapServer 4.6). MapServer defaults to *application/vnd.ogc.se_inimage* (the exception will be in a picture format).  You can check the GetCapabilities of the server to see what formats are available for exceptions.  The *application/vnd.ogc.se_inimage* exception format is actually a non-required exception format in the WMS 1.1.1 spec, so there are servers out there which don't support this format. In that case you would use:"
msgstr ""

#: ../../../en/ogc/wms_client.txt:308
# ebda9a3a773e482abe8efce9d34ff307
msgid "Which would return this xml exception in the MS_ERRORFILE:"
msgstr ""

#: ../../../en/ogc/wms_client.txt:324
# cbd5d31e0cd94a5e838a92dfc07bdd2d
msgid "**\"wms_force_separate_request\" metadata** - set this to \"1\" to force this WMS layer to be requested using its own separate GetMap request. By default MapServer will try to merge multiple adjacent WMS layers from the same server into a single multi-layer GetMap request to reduce the load on remote servers and improve response time. This metadata is used to bypass that behavior."
msgstr ""

#: ../../../en/ogc/wms_client.txt:330
# aae7835fe32a47e195525ff507518ae5
msgid "**\"wms_formatlist\" metadata** - comma-separated list of image formats supported by the remote WMS server. Note that wms_formatlist is used only if wms_format is not set. If both wms_format and wms_formatlist are provided then wms_format takes precedence."
msgstr ""

#: ../../../en/ogc/wms_client.txt:335
# 0abb8b837787425988ee85be929bbd8d
msgid "**\"wms_latlonboundingbox\" metadata** - the bounding box of this layer in geographic coordinates in the format \"lon_min lat_min lon_max lat_max\". If it is set then MapServer will request the layer only when the map view overlaps that bounding box. You normally get this from the server's capabilities output."
msgstr ""

#: ../../../en/ogc/wms_client.txt:347
# 54129007b98c4e13878ca2bb171a3c97
msgid "**\"wms_proxy_auth_type\" metadata** - the authorization type to use for a proxy connection.  Supported types include:"
msgstr ""

#: ../../../en/ogc/wms_client.txt:362
# 5232d91ca2bc49c1bd45c74042587d7b
msgid "**\"wms_proxy_host\" metadata** - the hostname of the proxy to use, in \"dot-quad\" format, with an optional port component (e.g. '192.168.2.10:8080')."
msgstr ""

#: ../../../en/ogc/wms_client.txt:371
# 627c71faee034732b55dc87a392e5923
msgid "**\"wms_proxy_port\" metadata** - the port to use for a proxy connection."
msgstr ""

#: ../../../en/ogc/wms_client.txt:380
# aff7fc3c9c014e939dfc15d46636aea8
msgid "**\"wms_proxy_type\" metadata** - the type of the proxy connection.  Valid values are 'http' and 'socks5', which are case sensitive."
msgstr ""

#: ../../../en/ogc/wms_client.txt:389
# ca07b5fb7ef0485cb07663a71c0b2596
msgid "**\"wms_proxy_username\" metadata** - msEncrypt-style string for a proxy connection.  Empty strings are also accepted."
msgstr ""

#: ../../../en/ogc/wms_client.txt:399
# 11f47bc0809349faa75c2467da4ae0d9
msgid "**\"wms_sld_body\" metadata** -  can be used to specify an inline SLD document."
