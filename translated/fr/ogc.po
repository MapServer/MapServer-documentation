#: ../../../en/ogc/wms_server.txt:432
# 66a0e67142f84ca6a0122a43131d0f46
msgid "**WIDTH=output_width:** Width in pixels of map picture."
msgstr ""

#: ../../../en/ogc/wms_server.txt:434
# 24216b43482a4b8cb7641c1f83a95814
msgid "**HEIGHT=output_height:** Height in pixels of map picture."
msgstr ""

#: ../../../en/ogc/wms_server.txt:436
# 24ab4c4c7836406eaf15c99df0facade
msgid "**FORMAT=output_format:** Output format of map."
msgstr ""

#: ../../../en/ogc/wms_server.txt:439
# e64dabbf99054cdd9fcaaeaa92f038e2
msgid "WMS Servers only advertise supported formats that are part of the gd / gdal libraries."
msgstr ""

