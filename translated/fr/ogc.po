#: ../../../en/ogc/wms_server.txt:639
# 0fde749524c44d4db447fce599a3fc2b
msgid "e.g, the URL wmsmap?mode=map will be rewritten as"
msgstr ""

#: ../../../en/ogc/wms_server.txt:642
# 545ce2a209c54293a0f4795a254fc278
msgid "::"
msgstr ""

